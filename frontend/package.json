{
    "name": "frontend",
    "version": "1.0.0",
    "scripts": {
        "build": "NODE_ENV=production node build.js",
        "clean:build": "rimraf ./build",
        "copy:backend": "rimraf ../backend/public && cp -r ./build ../backend/public",
        "coverage": "jest --coverage",
        "cypress": "cypress open",
        "cypress:run": "cypress run",
        "dev": "NODE_ENV=development node build.js --watch --reload",
        "dev:noreload": "NODE_ENV=development node build.js --watch",
        "format": "pnpm prettier && pnpm lint:fix",
        "lint": "node --no-deprecation ./node_modules/eslint/bin/eslint.js --max-warnings=0 --no-error-on-unmatched-pattern ./src",
        "lint:fix": "pnpm lint --fix",
        "prettier": "npx prettier ./src --write",
        "prod": "pnpm build && pnpm copy:backend",
        "test": "jest",
        "test:all": "pnpm test && pnpm cypress:run",
        "test:changed": "jest --onlyChanged",
        "test:failing": "jest --onlyFailures",
        "test:update": "jest --updateSnapshot",
        "test:watch": "jest --watch",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@mui/icons-material": "^7.3.6",
        "@mui/material": "^7.3.6",
        "@reduxjs/toolkit": "^1.9.7",
        "@types/react": "^17.0.90",
        "@types/react-dom": "^17.0.26",
        "axios": "^1.13.2",
        "classnames": "^2.5.1",
        "date-fns": "^3.6.0",
        "disqus-react": "^1.1.7",
        "esbuild": "^0.27.2",
        "fs-extra": "^11.3.3",
        "history": "^5.3.0",
        "lodash": "^4.17.21",
        "lodash.clonedeep": "^4.5.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-helmet": "^6.1.0",
        "react-redux": "^8.1.3",
        "react-router-dom": "^5.3.4",
        "react-swipeable-views": "^0.14.1",
        "redux": "^4.2.1",
        "toastr": "^2.1.4"
    },
    "devDependencies": {
        "@babel/core": "^7.28.5",
        "@babel/preset-env": "^7.28.5",
        "@babel/preset-react": "^7.28.5",
        "@eslint/compat": "^1.4.1",
        "@eslint/js": "^9.39.2",
        "@mui/codemod": "^7.3.6",
        "@testing-library/cypress": "^10.1.0",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^12.1.5",
        "@testing-library/react-hooks": "^8.0.1",
        "@testing-library/user-event": "^14.6.1",
        "@types/jest": "^29.5.14",
        "@types/lodash": "^4.17.21",
        "@types/node": "^18.19.130",
        "@types/react-helmet": "^6.1.11",
        "@types/react-router-dom": "^5.3.3",
        "@types/react-swipeable-views": "^0.13.6",
        "@types/redux-mock-store": "^1.5.0",
        "babel-jest": "^30.2.0",
        "cypress": "^15.8.1",
        "esbuild-sass-plugin": "^3.3.1",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-compat": "^5.0.0",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-unused-imports": "^4.3.0",
        "globals": "^15.15.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^30.2.0",
        "prettier": "3.3.2",
        "redux-mock-store": "^1.5.5",
        "rimraf": "^5.0.10",
        "servor": "^4.0.2",
        "ts-jest": "^29.4.6",
        "typescript": "^5.5.4",
        "typescript-eslint": "^7.18.0"
    },
    "pnpm": {
        "overrides": {
            "@babel/runtime": ">=7.26.10"
        }
    }
}
