{
  "name": "frontend",
  "version": "1.0.0",
  "scripts": {
    "clean:build": "rimraf ./build",
    "coverage": "jest --coverage",
    "dev": "NODE_ENV=development node build.js --watch --reload",
    "format": "pnpm prettier && pnpm lint:fix",
    "lint": "node --no-deprecation ./node_modules/eslint/bin/eslint.js --max-warnings=0 --no-error-on-unmatched-pattern ./src",
    "lint:fix": "pnpm lint --fix",
    "prettier": "npx prettier ./src --write",
    "prod": "NODE_ENV=production node build.js",
    "test": "jest",
    "test:changed": "jest --onlyChanged",
    "test:failing": "jest --onlyFailures",
    "test:update": "jest --updateSnapshot",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@reduxjs/toolkit": "^1.8.3",
    "@types/react": "^17.0.47",
    "@types/react-dom": "^18.0.6",
    "axios": "^1.7.2",
    "classnames": "^2.5.1",
    "date-fns": "^3.6.0",
    "disqus-react": "^1.1.5",
    "esbuild": "^0.21.5",
    "fs-extra": "^11.2.0",
    "history": "^5.3.0",
    "lodash": "^4.17.21",
    "lodash.clonedeep": "^4.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^5.2.0",
    "react-swipeable-views": "^0.14.0",
    "redux": "^4.2.0",
    "toastr": "^2.1.4",
    "typescript": "^5.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@eslint/compat": "^1.1.0",
    "@eslint/js": "^9.6.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.0.1",
    "@types/react-router-dom": "^5.3.3",
    "babel-jest": "^30.2.0",
    "esbuild-sass-plugin": "^3.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-compat": "^5.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^15.8.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "3.3.2",
    "rimraf": "^5.0.7",
    "servor": "^4.0.2",
    "ts-jest": "^29.4.6",
    "typescript-eslint": "^7.15.0"
  }
}
